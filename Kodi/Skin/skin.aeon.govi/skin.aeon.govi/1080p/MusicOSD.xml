<?xml version="1.0" encoding="UTF-8"?>
<window>
	<onload condition="Skin.HasSetting(Disable.MusicPlayerControlButtons)">SetFocus(703)</onload>
	<onload condition="!Skin.HasSetting(Disable.MusicPlayerControlButtons)">SetFocus(602)</onload>
	<controls>
		<control type="group">
			<animation effect="slide" end="0,-120" time="300" tween="quadratic" condition="Window.IsVisible(selectdialog)">Conditional</animation>
			<animation effect="slide" start="0,-120" end="0,0" time="300" tween="quadratic">WindowOpen</animation>
			<animation effect="slide" end="0,-120" start="0,0" time="300" tween="quadratic">WindowClose</animation>
			<control type="image">
				<left>-5%</left>
				<top>-30</top>
				<width>110%</width>
				<height>147</height>
				<texture border="40" flipy="true" colordiffuse="E6FFFFFF">dialogs/default/bg.png</texture>
			</control>
			<control type="grouplist" id="202">
				<height>100</height>
				<width>100%</width>
				<align>center</align>
				<scrolltime tween="sine">200</scrolltime>
				<orientation>horizontal</orientation>
				<onup>706</onup>
				<ondown>802</ondown>
				<onleft>202</onleft>
				<onright>202</onright>
				<control type="button" id="802">
					<texturefocus>icons/osd/blue/OSDRatingFO.png</texturefocus>
					<texturenofocus>icons/osd/blue/OSDRatingNF.png</texturenofocus>
					<label />
					<width>100</width>
					<height>100</height>
					<font />
					<onclick>SetRating</onclick>
				</control>
				<control type="button" id="700">
					<texturefocus>icons/osd/blue/OSDPlaylistFO.png</texturefocus>
					<texturenofocus>icons/osd/blue/OSDPlaylistNF.png</texturenofocus>
					<label />
					<width>100</width>
					<height>100</height>
					<font />
					<onclick>Close</onclick>
					<onclick>ActivateWindow(MusicPlaylist)</onclick>
				</control>
				<control type="button" id="500">
					<texturefocus>icons/osd/blue/OSDVizFO.png</texturefocus>
					<texturenofocus>icons/osd/blue/OSDVizNF.png</texturenofocus>
					<label />
					<width>100</width>
					<height>100</height>
					<font />
				</control>
				<control type="button" id="702">
					<texturefocus>icons/osd/blue/OSDSettingsFO.png</texturefocus>
					<texturenofocus>icons/osd/blue/OSDSettingsNF.png</texturenofocus>
					<label />
					<width>100</width>
					<height>100</height>
					<font />
					<onclick>Addon.Default.OpenSettings(xbmc.player.musicviz)</onclick>
					<visible>Visualisation.Enabled</visible>
				</control>
				<control type="button" id="705">
					<texturefocus>icons/osd/blue/OSDVideoFO.png</texturefocus>
					<texturenofocus>icons/osd/blue/OSDVideoNF.png</texturenofocus>
					<width>100</width>
					<height>100</height>
					<font />
					<onclick>ActivateWindow(visualisationpresetlist)</onclick>
					<visible>Visualisation.HasPresets</visible>
				</control>
				<control type="button" id="600">
					<texturefocus>icons/osd/blue/OSDPrevTrackFO.png</texturefocus>
					<texturenofocus>icons/osd/blue/OSDPrevTrackNF.png</texturenofocus>
					<label />
					<width>100</width>
					<height>100</height>
					<font />
					<onclick>PlayerControl(Previous)</onclick>
					<visible>!Skin.HasSetting(Disable.MusicPlayerControlButtons)</visible>
				</control>
				<control type="button" id="601">
					<texturefocus>icons/osd/blue/OSDRewindFO.png</texturefocus>
					<texturenofocus>icons/osd/blue/OSDRewindNF.png</texturenofocus>
					<label />
					<width>100</width>
					<height>100</height>
					<font />
					<onclick>PlayerControl(Rewind)</onclick>
					<visible>!Skin.HasSetting(Disable.MusicPlayerControlButtons)</visible>
				</control>
				<control type="button" id="603">
					<texturefocus>icons/osd/blue/OSDStopFO.png</texturefocus>
					<texturenofocus>icons/osd/blue/OSDStopNF.png</texturenofocus>
					<label />
					<width>100</width>
					<height>100</height>
					<font />
					<onclick>PlayerControl(Stop)</onclick>
					<visible>!Skin.HasSetting(Disable.MusicPlayerControlButtons)</visible>
				</control>
				<control type="togglebutton" id="602">
					<texturefocus>icons/osd/blue/OSDPauseFO.png</texturefocus>
					<texturenofocus>icons/osd/blue/OSDPauseNF.png</texturenofocus>
					<alttexturefocus>icons/osd/blue/OSDPlayFO.png</alttexturefocus>
					<alttexturenofocus>icons/osd/blue/OSDPlayNF.png</alttexturenofocus>
					<label />
					<altlabel>208</altlabel>
					<width>100</width>
					<height>100</height>
					<font />
					<usealttexture>Player.Paused | Player.Forwarding | Player.Rewinding</usealttexture>
					<onclick>PlayerControl(Play)</onclick>
					<visible>!Skin.HasSetting(Disable.MusicPlayerControlButtons)</visible>
				</control>
				<control type="button" id="604">
					<texturefocus>icons/osd/blue/OSDForwardFO.png</texturefocus>
					<texturenofocus>icons/osd/blue/OSDForwardNF.png</texturenofocus>
					<label />
					<width>100</width>
					<height>100</height>
					<font />
					<onclick>PlayerControl(Forward)</onclick>
					<visible>!Skin.HasSetting(Disable.MusicPlayerControlButtons)</visible>
				</control>
				<control type="button" id="605">
					<texturefocus>icons/osd/blue/OSDNextTrackFO.png</texturefocus>
					<texturenofocus>icons/osd/blue/OSDNextTrackNF.png</texturenofocus>
					<label />
					<width>100</width>
					<height>100</height>
					<font />
					<onclick>PlayerControl(Next)</onclick>
					<visible>!Skin.HasSetting(Disable.MusicPlayerControlButtons)</visible>
				</control>
				<control type="button" id="703">
					<texturefocus>icons/osd/blue/OSDLyricsFO.png</texturefocus>
					<texturenofocus>icons/osd/blue/OSDLyricsNF.png</texturenofocus>
					<label />
					<width>100</width>
					<height>100</height>
					<font />
					<onclick>Close</onclick>
					<onclick>RunScript(script.cu.lrclyrics)</onclick>
				</control>
				<control type="togglebutton" id="704">
					<texturefocus>icons/osd/blue/OSDRandomOffFO.png</texturefocus>
					<texturenofocus>icons/osd/blue/OSDRandomOffNF.png</texturenofocus>
					<alttexturefocus>icons/osd/blue/OSDRandomOnFO.png</alttexturefocus>
					<alttexturenofocus>icons/osd/blue/OSDRandomOnNF.png</alttexturenofocus>
					<label />
					<width>100</width>
					<height>100</height>
					<font />
					<usealttexture>Playlist.IsRandom</usealttexture>
					<onclick>PlayerControl(Random)</onclick>
					<visible>!MusicPartyMode.Enabled</visible>
				</control>
				<control type="button" id="707">
					<width>0.1</width>
					<height>0.1</height>
					<texturefocus />
					<texturenofocus />
					<label>$LOCALIZE[486]$INFO[Playlist.Repeat, : ]</label>
					<font />
					<onclick>PlayerControl(Repeat)</onclick>
					<visible>!MusicPartyMode.Enabled</visible>
				</control>
				<control type="image" id="708">
					<texture>$VAR[MusicOSDRepeatButtonVar]</texture>
					<width>100</width>
					<height>100</height>
					<visible>!MusicPartyMode.Enabled</visible>
				</control>
				<control type="button" id="706">
					<texturefocus>icons/osd/blue/OSDSkinFO.png</texturefocus>
					<texturenofocus>icons/osd/blue/OSDSkinNF.png</texturenofocus>
					<label />
					<width>100</width>
					<height>100</height>
					<font />
					<onclick>Dialog.Close(musicosd)</onclick>
					<onclick>ActivateWindow(1120)</onclick>
					<onclick>SetProperty(MusicOSD.Settings,True,Home)</onclick>
				</control>
				<control type="button" id="710">
					<description>Switch player</description>
					<width>80</width>
					<height>80</height>
					<texturefocus>icons/osd/blue/osdswitchplayerfo.png</texturefocus>
					<texturenofocus>icons/osd/blue/osdswitchplayernf.png</texturenofocus>			
					<onclick>Dialog.Close(all,true)</onclick>
					<onclick>SwitchPlayer</onclick>				
				</control>
			</control>
		</control>
	</controls>
</window>
